resource_name = "Parallax_camera"
script/source = "extends Camera2D

var vel = 150
var shake = 0


func _ready():
	set_process(true)
	pass

func _process(delta):
	offset += Vector2(0, -1) * vel * delta
	
	if shake > 0:
		position = Vector2(cos(rad2deg(shake)), sin(rad2deg(shake))) * 2
		shake -= delta
		pass
	else:
		position = Vector2(0,0)
		pass
	pass

func shake():
	shake = 0.3
	pass"
