script/source = "extends CanvasLayer



func _ready():
	set_process_input(true)

func _input(event):
	
	if event.is_action_pressed(\"Pause\") && !event.is_echo():
		get_tree().paused = !get_tree().is_paused()
		
		if get_tree().is_paused():
			$Paused.show()
		else:
			$Paused.hide()
